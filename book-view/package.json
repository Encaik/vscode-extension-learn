{
  "name": "book-view",
  "displayName": "book-view",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:allBookList",
    "onCommand:book-view.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "book-view.helloWorld",
        "title": "Hello World"
      }
    ],
    "views": {
      "book-explorer": [
        {
          "id": "allBookList",
          "name": "全部作品人气排行榜"
        },
        {
          "id": "finishBookList",
          "name": "完本作品人气排行榜"
        },
        {
          "id": "list3",
          "name": "起点月票榜"
        },
        {
          "id": "list4",
          "name": "24小时热销榜"
        },
        {
          "id": "list5",
          "name": "阅读指数榜"
        },
        {
          "id": "list6",
          "name": "推荐票榜"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "book-explorer",
          "title": "小说阅读器",
          "icon": "src/assert/favicon.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.46.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "eslint": "^7.15.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.9"
  }
}
